@model List<Data.Entities.Ingredient>



<h1>Ingredients</h1>
<table class=table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Import Date</th>
            <th>Expiration Date</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var ingred in Model)
        {
            <tr>
                <td>@ingred.Id</td>
                <td>@ingred.Name</td>
                <td>@ingred.ImportDate</td>
                <td>@ingred.ExpirationDate</td>
                <td>@ingred.Price</td>
            </tr>
        }
    </tbody>
</table>